  <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= project.user.username if project.user %></td>
        <td><%= format_date(project.created_at) %></td>
        <td>
        <%= link_to "Show", admin_project_path(project), class: "btn btn-show" %>
        <%= link_to "Edit", edit_admin_project_path(project), class: "btn btn-edit" %>
        <%= link_to "Delete",
            admin_project_path(project),
            method: :delete,
            data: { confirm: "Are you sure?" },
            class: "btn btn-delete" %>
        </td>
  </tr>